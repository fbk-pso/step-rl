type Match;
type Fuse;
fluent boolean handfree;
fluent boolean light;
fluent boolean match_used(Match match);
fluent boolean fuse_mended(Fuse fuse);
constant float [0.0, 6.0] mend_fuse_duration;
action light_match(Match m) {
   duration >= 7 and duration <= 7;
   [ start ] (not match_used(m));
   [ start ] match_used(m) := true;
   [ start ] light := true;
   [ end ] light := false;
};
action mend_fuse(Fuse f) {
   duration >= mend_fuse_duration and duration <= mend_fuse_duration;
   [ start ] handfree;
   [ start, end ] light;
   [ start ] handfree := false;
   [ end ] fuse_mended(f) := true;
   [ end ] handfree := true;
};
instance Match m12, m13, m14, m15, m16, m17, m18, m19, m20, m21;
instance Fuse f12, f13, f14, f15, f16, f17, f18, f19, f20, f21;
[ start ] handfree := true;
[ start ] light := false;
[ start ] match_used(m12) := false;
[ start ] match_used(m13) := false;
[ start ] match_used(m14) := false;
[ start ] match_used(m15) := false;
[ start ] match_used(m16) := false;
[ start ] match_used(m17) := false;
[ start ] match_used(m18) := false;
[ start ] match_used(m19) := false;
[ start ] match_used(m20) := false;
[ start ] match_used(m21) := false;
[ start ] fuse_mended(f12) := false;
[ start ] fuse_mended(f13) := false;
[ start ] fuse_mended(f14) := false;
[ start ] fuse_mended(f15) := false;
[ start ] fuse_mended(f16) := false;
[ start ] fuse_mended(f17) := false;
[ start ] fuse_mended(f18) := false;
[ start ] fuse_mended(f19) := false;
[ start ] fuse_mended(f20) := false;
[ start ] fuse_mended(f21) := false;
mend_fuse_duration := (6/1);
[ end ] fuse_mended(f12);
[ end ] fuse_mended(f13);
[ end ] fuse_mended(f14);
[ end ] fuse_mended(f15);
[ end ] fuse_mended(f16);
[ end ] fuse_mended(f17);
[ end ] fuse_mended(f18);
[ end ] fuse_mended(f19);
[ end ] fuse_mended(f20);
[ end ] fuse_mended(f21);
